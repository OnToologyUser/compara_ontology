@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix sio: <http://semanticscience.org/resource/> .
@prefix vann: <http://purl.org/vocab/vann/> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .

dc:created a owl:AnnotationProperty .

dc:creator a owl:AnnotationProperty .

dc:description a owl:AnnotationProperty .

dc:license a owl:AnnotationProperty .

dc:title a owl:AnnotationProperty .

vann:preferredNamespacePrefix a owl:AnnotationProperty .

vann:preferredNamespaceUri a owl:AnnotationProperty .

sio:SIO_010302 a owl:ObjectProperty,
        owl:SymmetricProperty,
        owl:TransitiveProperty ;
    rdfs:label "s i o_010302" ;
    rdfs:domain sio:SIO_010035 ;
    rdfs:range sio:SIO_010035 .

rdfs:label a owl:DatatypeProperty ;
    rdfs:label "label" .

owl:priorVersion a owl:AnnotationProperty .

owl:versionIRI a owl:AnnotationProperty .

owl:versionInfo a owl:AnnotationProperty .

comp:InterspeciesParalog a owl:Class ;
    rdfs:label "Interspecies Paralog" ;
    rdfs:subClassOf comp:ParalogGroup ;
    owl:disjointWith comp:IntraspeciesParalog .

comp:SpeciesTrees a owl:Class ;
    rdfs:label "Species Trees" ;
    rdfs:subClassOf comp:PhylogeneticTree ;
    owl:disjointWith comp:GeneTrees .

<https://w3id.org/def/compara_ontology> a owl:Ontology ;
    dc:created "2021-01-01"^^xsd:date ;
    dc:creator "Jacob González Isa",
        "Jimena Martín Reina" ;
    dc:description "COMPARA (Comparative Analysis and Molecular Phylogenetics for Advanced Research) ontology aims to facilitate data sharing and interoperability among the scientific community working in evolution and comparative genomics.."@en ;
    dc:license <https://creativecommons.org/licenses/by/4.0/> ;
    dc:title "COMPARA ontology"@en ;
    vann:preferredNamespacePrefix "choosenprefix" ;
    vann:preferredNamespaceUri <https://w3id.org/example#> ;
    owl:priorVersion <https://w3id.org/example/1.0.0> ;
    owl:versionIRI <https://w3id.org/example/1.0.1> ;
    owl:versionInfo "0.0.1" ;
    mod:createdWith <https://chowlk.linkeddata.es/> .

dcterms:source a owl:ObjectProperty ;
    rdfs:label "source" ;
    rdfs:domain sio:SIO_000030 ;
    rdfs:range sio:SIO_000750 .

sio:SIO_000068 a owl:ObjectProperty ;
    rdfs:label "s i o_000068" ;
    rdfs:domain sio:SIO_010035 ;
    rdfs:range <http://semanticscience.org/resource/ SIO_010016> .

sio:SIO_000339 a owl:ObjectProperty ;
    rdfs:label "s i o_000339" ;
    rdfs:domain comp:PhylogeneticTree ;
    rdfs:range sio:SIO_010067 .

sio:SIO_000557 a owl:ObjectProperty ;
    rdfs:label "s i o_000557" ;
    rdfs:domain comp:HomologGroup ;
    rdfs:range comp:PhylogeneticTree .

sio:SIO_001278 a owl:ObjectProperty ;
    rdfs:label "s i o_001278" ;
    rdfs:domain sio:SIO_000101 ;
    rdfs:range sio:SIO_000160 .

sio:SIO_010015 a owl:Class ;
    rdfs:label "S I O_010015" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://semanticscience.org/resource/ SIO_010016> ;
            owl:onProperty sio:SIO_010079 ],
        sio:SIO_000030 .

sio:SIO_010079 a owl:ObjectProperty ;
    rdfs:label "s i o_010079" ;
    rdfs:domain sio:SIO_010015 ;
    rdfs:range <http://semanticscience.org/resource/ SIO_010016> .

comp:GeneTrees a owl:Class ;
    rdfs:label "Gene Trees" ;
    rdfs:subClassOf comp:PhylogeneticTree .

comp:IntraspeciesParalog a owl:Class ;
    rdfs:label "Intraspecies Paralog" ;
    rdfs:subClassOf comp:ParalogGroup .

comp:involves a owl:ObjectProperty ;
    rdfs:label "involves" ;
    rdfs:domain comp:HomologGroup ;
    rdfs:range sio:SIO_010035 .

obo:NCIT_C16607 a owl:Class ;
    rdfs:label "N C I T_ C16607" ;
    rdfs:subClassOf obo:NCIT_C17828 ;
    owl:disjointWith comp:Speciation .

obo:NCIT_C17828 a owl:Class ;
    rdfs:label "N C I T_ C17828" .

sio:SIO_000101 a owl:Class ;
    rdfs:label "S I O_000101" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom comp:PhylogeneticTree ;
            owl:onProperty sio:SIO_000229 ],
        [ a owl:Restriction ;
            owl:allValuesFrom sio:SIO_000160 ;
            owl:onProperty sio:SIO_001278 ],
        [ a owl:Restriction ;
            owl:allValuesFrom sio:SIO_010067 ;
            owl:onProperty sio:SIO_000229 ] .

sio:SIO_000160 a owl:Class ;
    rdfs:label "S I O_000160" .

sio:SIO_000229 a owl:ObjectProperty ;
    rdfs:label "s i o_000229" ;
    rdfs:domain sio:SIO_000101 ;
    rdfs:range sio:SIO_010067,
        comp:PhylogeneticTree .

sio:SIO_000244 a owl:ObjectProperty ;
    rdfs:label "s i o_000244" ;
    rdfs:domain sio:SIO_010067 ;
    rdfs:range sio:SIO_000030 .

sio:SIO_000750 a owl:Class ;
    rdfs:label "S I O_000750" .

sio:SIO_001157 a owl:ObjectProperty ;
    rdfs:label "s i o_001157" ;
    rdfs:domain comp:OrthologGroup,
        comp:ParalogGroup ;
    rdfs:range obo:NCIT_C16607,
        comp:Speciation .

comp:OrthologGroup a owl:Class ;
    rdfs:label "Ortholog Group" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom comp:Speciation ;
            owl:onProperty sio:SIO_001157 ],
        comp:HomologGroup .

comp:ParalogGroup a owl:Class ;
    rdfs:label "Paralog Group" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom obo:NCIT_C16607 ;
            owl:onProperty sio:SIO_001157 ],
        comp:HomologGroup ;
    owl:disjointWith comp:OrthologGroup .

comp:Speciation a owl:Class ;
    rdfs:label "Speciation" ;
    rdfs:subClassOf obo:NCIT_C17828 .

<http://semanticscience.org/resource/ SIO_010016> a owl:Class ;
    rdfs:label "  S I O_010016" ;
    rdfs:subClassOf sio:SIO_000030 .

comp:HomologGroup a owl:Class ;
    rdfs:label "Homolog Group" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom comp:PhylogeneticTree ;
            owl:onProperty sio:SIO_000557 ],
        [ a owl:Restriction ;
            owl:allValuesFrom sio:SIO_010035 ;
            owl:onProperty comp:involves ] .

sio:SIO_000030 a owl:Class ;
    rdfs:label "S I O_000030" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom sio:SIO_000750 ;
            owl:onProperty dcterms:source ] .

sio:SIO_010035 a owl:Class ;
    rdfs:label "S I O_010035" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom <http://semanticscience.org/resource/ SIO_010016> ;
            owl:onProperty sio:SIO_000068 ] .

sio:SIO_010067 a owl:Class ;
    rdfs:label "S I O_010067" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom sio:SIO_000030 ;
            owl:onProperty sio:SIO_000244 ],
        [ a owl:Restriction ;
            owl:minCardinality "3"^^xsd:nonNegativeInteger ;
            owl:onProperty sio:SIO_000244 ] .

comp:PhylogeneticTree a owl:Class ;
    rdfs:label "Phylogenetic Tree" ;
    rdfs:subClassOf [ a owl:Restriction ;
            owl:allValuesFrom sio:SIO_010067 ;
            owl:onProperty sio:SIO_000339 ] .

